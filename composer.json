{
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "php-ai/php-ml": "^0.4.1",
        "symfony/dotenv": "^3.3",
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\":"app/", 
            "Core\\":"core/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
